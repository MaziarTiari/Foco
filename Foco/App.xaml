<Application x:Class="Foco.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Foco"
             Startup="OnAppStart"
             Exit="OnAppExit">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/resource-dictionaries/GeneralStyleDictionary.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/resource-dictionaries/IconButtonDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
